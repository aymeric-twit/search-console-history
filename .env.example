# Google OAuth2 Credentials
# Créer sur https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-client-secret
GOOGLE_REDIRECT_URI=http://localhost:8080/auth/callback

# Base de données MySQL
DB_HOST=127.0.0.1
DB_PORT=3306
DB_NAME=search_console
DB_USER=root
DB_PASSWORD=

# Application
APP_URL=http://localhost:8080
APP_ENV=development
APP_TIMEZONE=Europe/Paris

# Sync settings
# Nombre de jours à synchroniser en arrière (max ~480 pour les 16 mois)
SYNC_DAYS_BACK=480
# dataState: "final" pour données validées, "all" pour données fraîches incluses
SYNC_DATA_STATE=all
# Types de recherche à synchroniser (séparés par des virgules)
SYNC_SEARCH_TYPES=web,image,video
# Limite de lignes par requête API (max 25000)
SYNC_ROW_LIMIT=25000
# Nombre max de tentatives en cas d'erreur API
SYNC_MAX_RETRIES=3
# Délai entre les tentatives (secondes)
SYNC_RETRY_DELAY=5
